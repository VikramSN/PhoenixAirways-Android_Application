<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoenixairways"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.phoenixairways.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.phoenixairways.Register"
            android:label="@string/title_activity_register"
            android:noHistory="true" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.HomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.Admin"
            android:label="@string/title_activity_admin" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.FAQ"
            android:label="@string/title_activity_faq" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.ContactUs"
            android:label="@string/title_activity_contact_us" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.Products"
            android:label="@string/title_activity_products" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.Pone"
            android:label="@string/title_activity_pone" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.UploadProduct"
            android:label="@string/title_activity_upload_product" >
        </activity>
        <activity
            android:name="com.example.phoenixairways.ErrorPage"
            android:label="@string/title_activity_error_page" >
        </activity>
    </application>

</manifest>
